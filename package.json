{
  "name": "debugging_js",
  "version": "1.0.0",
  "description": "Setup of Dev environment and practicing testing / debugging",
  "main": "index.js",
  "author": "Darrell Washington",
  "license": "MIT",
  "scripts": {
    "clean-dist": "rimraf ./dist && mkdir dist",
    "dev-serve": "lite-server -c ./bs-config.js",
    "ngrok": " ~/Downloads/./ngrok http 8000",
    "predev-ngrok": "babel-node ./startMessage.js",
    "dev-ngrok": "npm-run-all --parallel dev-serve ngrok",
    "dev-build": "webpack -w --mode=development --config webpack.dev.js",
    "preprod-build": "npm-run-all clean-dist lint test",
    "prod-build": "webpack --mode=production --config webpack.prod.js",
    "webpack-serve": "npm-run-all --parallel dev-build dev-serve",
    "webpack-prod-serve": "npm-run-all --sequential prod-build dev-ngrok",
    "lint": "./node_modules/.bin/eslint ./ src webpack.*.*",
    "debug": "babel-node --inspect src/*.js",
    "dev-debug": "npm-run-all --parallel debug dev-serve",
    "pretest": "yarn lint",
    "test": "mocha --reporter progress assets/testSetup.js \"src/**/*.spec.js\""
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "express": "^4.16.3",
    "jsdom": "^11.8.0",
    "mocha": "^5.1.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "sinon": "^5.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-stage-3": "^6.24.1",
    "chalk": "^2.4.0",
    "clean-webpack-plugin": "^0.1.19",
    "eslint-plugin-mocha": "^5.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack-cli": "^2.0.14",
    "webpack-merge": "^4.1.2"
  }
}
